                            <a class="MainMenuLevel1 HasUL" href="#">
                                <i class="fa fa-columns" aria-hidden="true"></i>
                                <span>Page layouts</span>
                            </a>
                            <ul>
                                <li>
                                    <a class="MainMenuLevel2 HasUL" href="#">
                                        <span>Fixed navbar</span>
                                    </a>
                                    <ul>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 1</span></a></li>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 2</span></a></li>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 3</span></a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a class="MainMenuLevel2 HasUL" href="#">
                                        <span>Boxed with default sidebar</span>
                                    </a>

                                    <ul>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 1</span></a></li>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 2</span></a></li>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 3</span></a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a class="MainMenuLevel2 HasUL" href="#">
                                        <span>Boxed with mini sidebar</span>
                                    </a>
                                    <ul>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 1</span></a></li>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 2</span></a></li>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 3</span></a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a class="MainMenuLevel2 HasUL" href="#">
                                        <span>Boxed full width</span>
                                    </a>
                                    <ul>
                                        <li><a class="MainMenuLevel3" href="#"><span>2.1. 1</span></a></li>
